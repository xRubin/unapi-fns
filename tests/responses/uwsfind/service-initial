<script type="text/javascript">
    var CAPTCHA_VERSION = '1';
    $(function () {
        $(".captcha-field a").click(function () {
            var $field = $(this).closest(".captcha-field");
            $.get("/static/captcha.html?" + (new Date()).getTime())
                    .success(function (response) {
                        $field.find("img").attr("src", "/static/captcha.html?r=" + (new Date()).getTime() + "&a=" + response + "&version=" + CAPTCHA_VERSION);
                        $field.find(":hidden").val(response);
                        var $inp = $field.find(":text");
                        $inp.focus().val("");
                        $inp.removeClass("input-error");
                        $field.find(".field-errors").remove();
                    })
                    .error(function () {
                        alert("Ошибка при получении картинки. Попробуйте еще раз");
                    });
            return false;
        });
    });
</script>

